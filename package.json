{
  "name": "nebb-calculator",
  "version": "1.0.0",
  "description": "Calculator for NEBB certified professionals",
  "main": "index.js",
  "scripts": {
    "start": "CI=true DISABLE_WATCHMAN=true expo start",
    "android": "CI=true DISABLE_WATCHMAN=true expo start --android",
    "ios": "CI=true DISABLE_WATCHMAN=true expo start --ios",
    "web": "webpack serve --mode development --port 8080",
    "start:mobile": "CI=true DISABLE_WATCHMAN=true expo start",
    "start:web": "webpack serve --mode development --port 8080",
    "build": "expo export",
    "build:web": "expo export --platform web --output-dir dist",
    "update:dist": "npm run build:web",
    "preview:dist": "http-server dist -p 4173 -c-1",
    "preview:web": "npm run preview:dist",
    "eject": "expo eject",
    "deploy": "vercel --prod",
    "test": "jest"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~49.0.0",
    "expo-clipboard": "~4.3.1",
    "expo-font": "~11.4.0",
    "expo-haptics": "~12.4.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.11.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "http-server": "^14.1.1",
    "jest": "^29.2.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.1.3",
    "webpack-cli": "^6.0.1"
  },
  "private": true
}
